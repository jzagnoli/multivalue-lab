$BASICTYPE "P"
PROGRAM MAKE.TOTP
PROGS = 'GET.BASIC.STACK PY.CHECK.MODULES PY.ERROR.CHECK TOTP.TWO.FACTOR.AUTH TOTP.TEST'
CONVERT ' ' TO @AM IN PROGS
DC = DCOUNT(PROGS,@AM)
ECNTR = 0
FOR I = 1 TO DC
  COMM = 'BASIC BP ':PROGS<I>
  CRT COMM
  EXECUTE COMM
  IF @SYSTEM.RETURN.CODE THEN ECNTR+=1
  COMM = 'CATALOG BP ':PROGS<I>:' DIRECT FORCE'
  CRT COMM
  EXECUTE COMM
NEXT I
CRT ECNTR:' errors'
END

